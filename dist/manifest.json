{
  "manifest_version": 2,
  "name": "GitHub board Collapsed",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/projects/*"
      ],
      "run_at": "document_end",
      "js": [
        "js/index.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "permissions": [],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
